package vendingmachine.controllor;

public interface VendingMachineActable {
	public void inputCoin(int CoinCnt);

	public int returnCoin();

	public void registerProduct(String nameDrink, int priceDrink, int stockDrink);

	public void deleteProduct(String nameDrink);

	public void reviseProduct(String nameDrink, String name2Change, int price2Change);

	public void showProducts();

	public void pickProduct(String name2Pick);
}
